{
  "name": "stencil2",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil App Starter",
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "start.01": "SRC_DIR=01 npm start",
    "start.02": "SRC_DIR=02 npm start",
    "start.03": "SRC_DIR=03 npm start",
    "start.04": "SRC_DIR=04 npm start",
    "test.01": "SRC_DIR=01 npm run test",
    "test.02": "SRC_DIR=02 npm run test",
    "test.03": "SRC_DIR=03 npm run test",
    "test.04": "SRC_DIR=04 npm run test",
    "test.watch.01": "SRC_DIR=01 npm run test.watch",
    "test.watch.02": "SRC_DIR=02 npm run test.watch",
    "test.watch.03": "SRC_DIR=03 npm run test.watch",
    "test.watch.04": "SRC_DIR=04 npm run test.watch",
    "test.solution.01": "IS_SOLUTION=true SRC_DIR=01 npm run test",
    "test.solution.02": "IS_SOLUTION=true SRC_DIR=02 npm run test",
    "test.solution.03": "IS_SOLUTION=true SRC_DIR=03 npm run test",
    "test.solution.04": "IS_SOLUTION=true SRC_DIR=04 npm run test",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "test.solution": "run-s test.solution.*"
  },
  "dependencies": {
    "@stencil/core": "~0.16.2",
    "@stencil/router": "~0.3.1"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "23.3.14",
    "@types/puppeteer": "1.12.3",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "1.8.0"
  }
}
